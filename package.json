    "test:functional": "NODE_ENV=dev jest --projects ./test --runInBand --forceExit ",
    "test:integration": "NODE_ENV=dev jest --projects './test/integration' --forceExit --runInBand",
    "test:unit": "NODE_ENV=dev jest",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
