  "scripts": {
    "build": "tsc",
    "prebuild": "copyfiles src/views/*.* src/public/**/*.* dist/",
    "prestart": "yarn typeorm migration:run",
    "start": "yarn build && cd ./dist/src && node index.js",
    "start:local": "NODE_ENV=dev ts-node-dev  --transpile-only  'src/index.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
